          {{ SlideSVGOne.content()}}
        </div> #}
        <div class="swiper-slide">
          {{ SlideSVGTwo.content()}}
        </div>
        <div class="swiper-slide">
          {{ MainSlide.content({
            text: "Codeando Animaciones"
          })}}
        </div>
        <div class="swiper-slide">
          {{ MainSlideBis.content({
            text: "Animación <span>=</span> Control"
          }) }}
